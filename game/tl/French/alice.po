# game/events/alice.rpy:441 #-#-# translate crowdin alice_shower_spider_23328126:
msgid "[Alice_03] Ну всё, я пошла... Только не забудь паука вышвырнуть из ванной, хорошо?!"
msgstr "[Alice_03] C'est ça, je m'en vais... N'oublies pas de virer l'araignée de la salle de bain, d'accord?!"

# game/events/alice.rpy:442 #-#-# translate crowdin alice_shower_spider_428606a5:
msgid "[Max_01] Да. Не забуду..."
msgstr "[Max_01] Oui. Je n'oublierais pas..."

# game/events/alice.rpy:450 #-#-# translate crowdin alice_shower_spider_666dd0d3:
msgid "[Alice_14] Макс!!! Ах ты... Ну-ка дай сюда полотенце!!!"
msgstr "[Alice_14] Max ! !! Tu... Donnes-moi cette serviette!!!"

# game/events/alice.rpy:453 #-#-# translate crowdin alice_shower_spider_35513e76:
