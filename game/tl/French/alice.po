# game/events/alice.rpy:389 #-#-# translate crowdin alice_shower_spider_3f8fc33f:
msgid "[Alice] {b}Алиса:{/b} А-а-а-а-а!!! Вот чёрт... Охренеть!"
msgstr "[Alice] {b}Alice:{/b} A-a-a-a-a-a!!! Voici l’enfer... Oh, putain!"

# game/events/alice.rpy:390 #-#-# translate crowdin alice_shower_spider_6b101d76:
msgid "[Max_02] {m}...подождать.{/m}"
