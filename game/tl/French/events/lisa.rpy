# game/events/lisa.rpy:1821# translate crowdin lisa_horor_movie_r_1dc372f8:
msgid "[Max_01]Ага. Спокойной ночи."
msgstr "[Max_01]Ouaip. Bonne nuit."

# game/events/lisa.rpy:1825# translate crowdin lisa_horor_movie_r_26ebf379:
msgid "[Lisa_13]Макс, мне кажется или у меня под ногой сейчас что-то увеличивается?"
