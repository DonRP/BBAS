# game/scripts_s1/s1_v91.rpy:749
msgid "{i}продолжить массаж с поцелуями в шею{/i}"
msgstr "{i}continuez le massage avec des baisers sur le cou."

# game/scripts_s1/s1_v91.rpy:764
msgid "{i}продолжить массаж и поцелуи{/i}"
msgstr "{i}continuer à se masser et à s'embrasser{\\r}."

# game/scripts_s1/s1_v91.rpy:792
