# game/scripts_s1/s1_v91.rpy:1342
msgid "{i}ласкать пальцами быстрее{/i}"
msgstr "{i}car car caresser les doigts plus rapidement{/i}"

# game/scripts_s1/s1_v91.rpy:1370
msgid "{i}ласкать мамину киску языком{/i}"
msgstr "{i}"

# game/scripts_s1/s1_v91.rpy:1376
msgid "{i}не останавливаться{/i}"
msgstr "{i}ne t'arrête pas{/i}"

# game/scripts_s1/s1_v91.rpy:1383
msgid "{i}проникнуть в маму пальцами{/i}"
msgstr "{i}pénétrer la mère avec ses doigts{/i}"

# game/scripts_s1/s1_v91.rpy:1389
msgid "{i}тоже кончить!{/i}"
msgstr "{i}cum, aussi !"

