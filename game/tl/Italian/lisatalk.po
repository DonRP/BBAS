# XX## game/dialogues/lisatalk.rpy:1085
msgid "Ты пока отдохни, я сам всё сделаю!"
msgstr ""

# XX## game/dialogues/lisatalk.rpy:1085
msgid "Ага... {i}(сделать ошибки){/i}"
msgstr ""

# XX## game/dialogues/lisatalk.rpy:1085
msgid "Давай я всё сделаю сам! {i}(без ошибок){/i}"
