# game/dialogues/alicetalk.rpy:3093 #-#-# translate crowdin massage_sunscreen_sit_and_wait_54c7faac:
msgid "[Alice_05] Тебе хватит. Не обольщайся..."
msgstr "[Alice_05] ti basta. Non fatevi ingannare..."

# game/dialogues/alicetalk.rpy:3094 #-#-# translate crowdin massage_sunscreen_sit_and_wait_de3d2863:
