msgstr "E perché indossi le mutandine?"

# game/dialogues/alicetalk.rpy:848
msgid "{i}стянуть с неё трусики и продолжить массаж{/i}"
msgstr "{i}togliersi le mutandine e continuare il massaggio{/i}"

# game/dialogues/alicetalk.rpy:922
